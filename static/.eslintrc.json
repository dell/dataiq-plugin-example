// This is ESLint's configuration file. See https://eslint.org/docs/user-guide/configuring
{
    "env": {
        "browser": true,
        "es6": true,
        "es2020": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jest-dom/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest-dom"
    ],
    "rules": {
    }
}
